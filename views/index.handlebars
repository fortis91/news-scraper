<div class="jumbotron text-center">
    {{!-- <div class="overlay">
    </div> --}}
    <div class="caption">
        <h1>News Scraper</h1>
        <p>Scaping The Washington Post</p>
    </div>
    <button type="button" class="btn btn-primary fload-sm-right" id="scrape">Scrape</button>

</div>
<div class="container">
        {{!-- <button type="button" class="btn btn-primary float-sm-right" id="scrape">Scrape!</button> --}}

    {{#each dbArticles}}
    <div class="row">
        <div class="col-12">
            <div class="card mt-1 mb-2">
                <div class="card-horizontal">
                    <div class="card-body">
                        {{!-- <a href={{link}} target="_blank" class="card-title">{{title}}</a> --}}
                        <p class="card-title">{{title}}</p>
                        <br>
                        <p>{{summary}}</p>
                        <a href={{link}} target="_blank" class="btn btn-primary float-sm-right btn-sm">Read</a>
                        <a href="/comments/{{_id}}" class="btn btn-primary float-sm-right btn-sm">Comments</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
    {{/each}}
</div>
<script type="text/javascript" src="/assets/js/news.js"></script>

